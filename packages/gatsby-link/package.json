{
  "name": "gatsby-link",
  "description": "An enhanced Link component for Gatsby sites with support for resource prefetching",
  "version": "5.0.0-next.2",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.modern.mjs",
  "types": "index.d.ts",
  "files": [
    "dist/*",
    "index.d.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "bundle": "node scripts/bundle.js",
    "build": "npm-run-all --npm-path npm -s build:cjs build:esm",
    "build:cjs": "node scripts/bundle.js --key=cjs",
    "build:esm": "node scripts/bundle.js --key=esm",
    "watch": "npm-run-all --npm-path npm -p watch:cjs watch:esm",
    "watch:cjs": "npm run build:cjs -- --watch=true",
    "watch:esm": "npm run build:esm -- --watch=true",
    "prepare": "cross-env NODE_ENV=production npm run clean && npm run build",
    "clean": "del-cli dist/*"
  },
  "dependencies": {
    "@types/reach__router": "^1.3.10",
    "gatsby-page-utils": "^3.0.0-next.2",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.7",
    "cross-env": "^7.0.3",
    "del-cli": "^5.0.0",
    "microbundle": "^0.15.1",
    "npm-run-all": "^4.1.5",
    "prepend-directive": "^1.1.0"
  },
  "peerDependencies": {
    "@gatsbyjs/reach-router": "^2.0.0-v2.0",
    "react": "^18.0.0 || ^0.0.0",
    "react-dom": "^18.0.0 || ^0.0.0"
  },
  "homepage": "https://github.com/gatsbyjs/gatsby/tree/master/packages/gatsby-link#readme",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby.git",
    "directory": "packages/gatsby-link"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
